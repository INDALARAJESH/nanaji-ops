                "efs:*",
                "ecs:*",
                "ecr:*",
                "ec2:*",
                "dynamodb:*",
                "codecommit:*",
                "codebuild:*",
                "cloudwatchevents:*",
                "cloudwatch:*",
                "cloudformation:*",
                "appflow:*",
                "apigateway:*",
                "acm:List*",
                "acm:Get*",
                "acm:Describe*"
